<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>

<div class="row">
  <div class="span6 offset3 offset">
    <%= form_for(@user) do |f| %>
        <%= render 'fields', f:f %>
        <div class = "center ">
        <%= f.submit "Create my account", class: "btn btn-large btn-lg btn-primary" %>
        </div>
    <% end %>
  </div>
</div>
